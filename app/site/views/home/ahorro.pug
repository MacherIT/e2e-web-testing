.cuerpo.flex.fmcolumn
  .flex.jcenter.acenter.izquierda.fmcolumn
    .main-titulo.mobileV
      span {{ahorro.titulo}}
    .reloj(style="background-image: url({{ahorro.reloj}})")
  .flex.jcenter.astart.fcolumn.derecha
    .titulo.mobileI
      span {{ahorro.titulo}}
    .flex.jbetween.acenter.items
      .flex.jstart.acenter.item(ng-repeat="item in ahorro.items track by $index")
        .icono(style="background-image: url({{item.icono}})")
        .flex.jcenter.astart.fcolumn
          .titulo
            span {{item.titulo}}
          .texto
            p {{item.texto}}
